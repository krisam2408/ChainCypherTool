<UserControl x:Class="ChainCypherTool.View.Partials.NavigationControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ChainCypherTool.View.Partials"
             mc:Ignorable="d" 
             DataContext="{Binding Terminal, Source={StaticResource Locator}}">
    <StackPanel DataContext="{Binding Navigation}" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="8">
        <ComboBox ItemsSource="{Binding NavDestinations}"></ComboBox>
    </StackPanel>
</UserControl>
